{
  "name": "luminara",
  "version": "0.3.0",
	"description": "Lightweight, framework-agnostic HTTP client with native fetch, retry strategies, backoff algorithms, and interceptors. Universal browser compatibility across React, Vue, Angular, Svelte, and vanilla JavaScript.",
  "type": "module",
  "module": "./src/index.js",
  "main": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "author": {
    "name": "Jonathan Miller",
    "email": "jonathan@miller28.com",
    "url": "https://www.linkedin.com/in/miller28/"
  },
  "keywords": [
		"fetch",
		"native-fetch",
		"http",
		"http-client",
		"ajax",
		"client",
		"browser",
		"framework-agnostic",
		"universal",
		"react",
		"vue",
		"angular",
		"svelte",
		"vanilla-js",
		"pure-javascript",
		"drivers",
		"ofetch-optional",
		"interceptors",
		"enhanced-interceptors",
		"deterministic-order",
		"mutable-context",
		"retry-aware",
		"retry",
		"backoff",
		"exponential-backoff",
		"fibonacci",
		"jitter",
		"plugins",
		"middleware",
		"timeout",
		"abort",
		"abort-controller",
		"promise"
	],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miller-28/luminara.git"
  },
  "bugs": {
    "url": "https://github.com/miller-28/luminara/issues"
  },
  "homepage": "https://github.com/miller-28/luminara#readme",
  "peerDependencies": {
    "ofetch": "^1.5.0"
  },
  "peerDependenciesMeta": {
    "ofetch": {
      "optional": true
    }
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
