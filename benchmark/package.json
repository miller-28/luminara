{
	"name": "@luminara/benchmarks",
	"version": "0.1.0",
	"description": "Performance benchmarks for Luminara HTTP client",
	"author": {
		"name": "Jonathan Miller",
		"email": "jonathan@miller28.com",
		"url": "https://www.linkedin.com/in/miller28/"
	},
	"private": true,
	"type": "module",
	"scripts": {
		"benchmark": "node --expose-gc node/index.js",
		"benchmark:memory": "node --expose-gc node/index.js --memory",
		"benchmark:core": "node --expose-gc node/index.js --category=core",
		"benchmark:orchestration": "node --expose-gc node/index.js --category=orchestration",
		"benchmark:driver": "node --expose-gc node/index.js --category=driver",
		"benchmark:features": "node --expose-gc node/index.js --category=features",
		"benchmark:integrated": "node --expose-gc node/index.js --category=integrated",
		"benchmark:report": "node scripts/generate-report.js",
		"benchmark:headless:quick": "node headless/quick-test.js",
		"benchmark:headless:full": "node headless/index.js"
	},
	"dependencies": {
		"tinybench": "^2.9.0",
		"chalk": "^5.3.0",
		"cli-table3": "^0.6.5",
		"commander": "^12.1.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.48.2"
	},
	"keywords": [
		"benchmark",
		"performance",
		"luminara",
		"http-client"
	],
	"license": "MIT"
}
